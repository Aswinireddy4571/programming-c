#include <iostream>
#include <string>

using namespace std;

class Student {
    string name;
public:
    Student(string n = "Unknown") {
        name = n;
    }
    string getName() {
        return name;
    }
};

int main() {
    Student s1("John");
    Student s2("Mary");
    Student s3;

    cout << "Student 1 name: " << s1.getName() << endl;
    cout << "Student 2 name: " << s2.getName() << endl;
    cout << "Student 3 name: " << s3.getName() << endl;

    return 0;
}
